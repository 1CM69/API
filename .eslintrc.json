{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "consistent-return": "off",
      "no-console": "off",
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "radix": "off",
      "prefer-destructuring": "off",
      "func-names": "off",
      "prefer-arrow-callback": "off",
      "no-else-return": "off",
      "no-unresolved": "off",
      "indent": ["error", 2],
      "max-len": ["error", 120]
  }
}